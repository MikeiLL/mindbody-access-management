{
	"name": "mikeill/mz-mbo-access",
	"description": "Grant Client Access Based on Mindbody Online API Credentials",
	"keywords": [
    "MBO",
    "mindbody",
    "wordpress",
    "plugin",
    "session"
  ],
	"homepage": "https://github.com/MikeiLL/mz-mbo-access",
	"license"          : "GPL-2.0-or-later",
	"minimum-stability": "dev",
	"authors": [
    {
      "name": "Mike iLL Kilmer",
      "email": "mike@mzoo.org",
      "homepage": "http://mzoo.org",
      "role": "Developer"
    }
   ],
	"type": "wordpress-plugin",
	"require": {
		"php": ">=7.1",
		"composer/installers": "~1.0",
		"ericmann/sessionz": "^0.3",
		"squizlabs/php_codesniffer": "*",
    "wp-coding-standards/wpcs": "*"
	},
	"config": {
	  "optimize-autoloader": true,
	  "preferred-install": "dist",
	  "sort-packages": true
	},
	"scripts": {
		"lint": [
		  "phpcs --extensions=php --standard=PSR12 src *.php"
		],
    "fix": ["phpcbf --extensions=php --standard=PSR12 src *.php"]
	},
	"autoload": {
		"psr-4": {
			"MzMboAccess\\": "src",
			"EAMann\\": "libraries/wp-sessions",
			"EAMann\\WPSession\\": "libraries/wp-sessions",
			"EAMann\\WPSession\\Objects\\": "libraries/wp-sessions"
		}
	}
}
